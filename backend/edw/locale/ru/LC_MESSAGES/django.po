msgstr ""
"Перейдите по ссылке %(activation_link)s для активации аккаунта %(email)s."

#: templates/edw/email/activate-account-body.txt:7
#, python-format
msgid "Link is valid for %(expiration_days)s days."
msgstr "Ссылка будет активна %(expiration_days)s дней."

#: templates/edw/email/activate-account-body.txt:9
#: templates/edw/email/register-user-body.txt:8
#: templates/edw/email/reset-password-body.txt:8
msgid "Thanks for using our site!"
msgstr "Спасибо, что используете наш сайт."

#: templates/edw/email/activate-account-body.txt:11
#: templates/edw/email/register-user-body.txt:10
#: templates/edw/email/reset-password-body.txt:10
#, python-format
