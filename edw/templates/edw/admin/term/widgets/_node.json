{% load i18n edw_tags %}{% templatetag openbrace %}{% block content %}{% comment %}
{% endcomment %}"label": "<img src='/static/edw/img/admin/semantic_rules/{{ node.semantic_rule }}.png'><input class='term_set_checkbox' type='checkbox' name='{{ name }}' value='{{ node.id }}' id='id_{{ name }}_{{ node.id }}'{% block checkbox_extra %}{% endblock %}>{% comment %}
{% endcomment %}<span class='term semantic_rule_{{ node.semantic_rule }} {% if node.active %}on{% else %}off{% endif %}{% comment %}
{% endcomment %}{% if node.attributes|bitwise_and:1 %} is-characteristic{% endif %}{% comment %}
{% endcomment %}{% if node.attributes|bitwise_and:2 %} is-mark{% endif %}{% comment %}
{% endcomment %}{% if node.attributes|bitwise_and:4 %} is-relation{% endif %}'>{% comment %}
{% endcomment %}<i class='icons fa fa-link is_relation' title='{% trans 'Relation' %}'></i>{% comment %}
{% endcomment %}<i class='icons fa fa-tags is_mark' title='{% trans 'Mark' %}'></i>{% comment %}
{% endcomment %}<i class='icons fa fa-list is_characteristic' title='{% trans 'Characteristic' %}'></i>{% comment %}
{% endcomment %}<i class='icons fa fa-power-off' title='{% trans 'Active' %}'></i>{% comment %}
{% endcomment %}<label for='id_{{ name }}_{{ node.id }}' class='title' title='{{ node.name }}'>{{ node.name }}</label>",{% comment %}
{% endcomment %}"id": {{ node.id }}{% block load_on_demand %}{% if not node.is_leaf %}, "load_on_demand": true{% endif %}{% endblock %}{% endblock %}{% templatetag closebrace %}